{
    "name": "wyattcast44/gsuite",
    "description": "Helps manage your GSuite Account from your Laravel application",
    "keywords": [
        "wyattcast44",
        "gsuite"
    ],
    "homepage": "https://github.com/wyattcast44/gsuite",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Wyatt",
            "email": "wyatt.castaneda@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "google/apiclient": "^2.2",
        "illuminate/support": "5.8.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Wyattcast44\\GSuite\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wyattcast44\\GSuite\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wyattcast44\\GSuite\\GSuiteServiceProvider"
            ],
            "aliases": {
                "GoogleClient": "Wyattcast44\\GSuite\\Facades\\GoogleClientFacade",
                "GoogleServicesClient": "Wyattcast44\\GSuite\\Facades\\GoogleServicesClientFacade",
                "GSuite": "Wyattcast44\\GSuite\\Facades\\GSuiteFacade",
                "GSuiteAccount": "Wyattcast44\\GSuite\\Facades\\GSuiteAccountFacade"
            }
        }
    }
}
